
# Requre quartus project
package require ::quartus::project

# Set pin locations for LCD on GPIO 0
set_location_assignment PIN_AF26 -to LT24Data[0]
set_location_assignment PIN_AF25 -to LT24Data[1]
set_location_assignment PIN_AE24 -to LT24Data[2]
set_location_assignment PIN_AE23 -to LT24Data[3]
set_location_assignment PIN_AJ27 -to LT24Data[4]
set_location_assignment PIN_AK29 -to LT24Data[5]
set_location_assignment PIN_AK28 -to LT24Data[6]
set_location_assignment PIN_AK27 -to LT24Data[7]
set_location_assignment PIN_AJ26 -to LT24Data[8]
set_location_assignment PIN_AK26 -to LT24Data[9]
set_location_assignment PIN_AH25 -to LT24Data[10]
set_location_assignment PIN_AJ25 -to LT24Data[11]
set_location_assignment PIN_AJ24 -to LT24Data[12]
set_location_assignment PIN_AK24 -to LT24Data[13]
set_location_assignment PIN_AG23 -to LT24Data[14]
set_location_assignment PIN_AK23 -to LT24Data[15]
set_location_assignment PIN_AD21 -to LT24Reset_n
set_location_assignment PIN_AH27 -to LT24RS
set_location_assignment PIN_AH23 -to LT24CS_n
set_location_assignment PIN_AG26 -to LT24Rd_n
set_location_assignment PIN_AH24 -to LT24Wr_n
